{
  "name": "ts-boilerplate",
  "version": "0.1.0",
  "description": "TS boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "nodemon",
    "build": "NODE_ENV=production webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:server": "NODE_ENV=production webpack --config webpack.config.server.js --mode production",
    "build:server:dev": "webpack --config webpack.config.server.js --mode development",
    "test": "nyc mocha './src/**/*.test.{ts,tsx}'",
    "clean": "rm -rf dist"
  },
  "author": "-",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@types/chai": "^4.1.7",
    "@types/enzyme": "^3.1.16",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/koa": "^2.0.46",
    "@types/koa-static-cache": "^5.1.0",
    "@types/koa-webpack": "^5.0.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.19",
    "@types/react": "^16.7.22",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.1",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack": "^4.4.24",
    "@types/webpack-dev-middleware": "^2.0.2",
    "classnames": "^2.2.6",
    "koa": "^2.7.0",
    "koa-webpack": "^5.1.1",
    "less": "^3.8.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.1.6",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/koa-mount": "^3.0.1",
    "@types/koa-static": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-file-loader": "^1.1.1",
    "chai": "^4.2.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "funfix": "^7.0.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0",
    "pirates": "^4.0.0",
    "react-hot-loader": "^4.3.12",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "nodemonConfig": {
    "watch": [
      "src/server"
    ],
    "ext": "ts,tsx,json",
    "ignore": [
      "dist"
    ],
    "exec": "ts-node ./src/server/index.dev.tsx"
  }
}
